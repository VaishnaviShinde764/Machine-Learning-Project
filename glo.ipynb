    "    cv2.normalize(hist, hist)\n",
    "    
    "    return hist.flatten()\n",
    "\n",
    
    "train_labels = os.listdir(train_path)\n",
    "\n",
   
    "train_labels.sort()\n",
    "print(train_labels)\n",
    "\n",
    
    "global_features = []\n",
    "labels          = []\n",
    "\n",
    
    "for training_name in train_labels:\n",
